local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TITLE", "DarkTheme")
local Tab = Window:NewTab("Auto")
local Section = Tab:NewSection("auto")
Section:NewButton("Mediaval Box", "ButtonInfo", function()
while wait (0) do
local args = {
    [1] = "Medieval_Box"
}

game:GetService("ReplicatedStorage").Remotes.Boxes.RequestBoxUnlock:InvokeServer(unpack(args))
end
    print("Clicked")
end)
Section:NewButton("Autumn", "ButtonInfo", function()
while wait (0) do
local args = {
    [1] = "Autumn_Box"
}

game:GetService("ReplicatedStorage").Remotes.Boxes.RequestBoxUnlock:InvokeServer(unpack(args))
end
    print("Clicked")
end)
