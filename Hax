while wait (0) do
local args = {
    [1] = "Autumn_Hat_Box"
}

game:GetService("ReplicatedStorage").Remotes.Boxes.RequestBoxUnlock:InvokeServer(unpack(args))
end
    print("Clicked")
end)
Section:NewButton("Australia", "ButtonInfo", function()
while wait (0) do
local args = {
    [1] = "Australia_Box"
}

game:GetService("ReplicatedStorage").Remotes.Boxes.RequestBoxUnlock:InvokeServer(unpack(args))
